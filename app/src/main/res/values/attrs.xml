<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CheckedImageView">
        <!-- Indicates the initial checked state of this image. -->
        <attr name="checked" format="boolean" />
    </declare-styleable>

    <attr name="interpolator" format="reference" />

    <declare-styleable name="InterpolatorView">
        <attr name="interpolator" />
    </declare-styleable>

    <declare-styleable name="AnimatedBorderView">
        <attr name="borderWidth" format="dimension" />
    </declare-styleable>

    <declare-styleable name="AnimatedStarView">
        <attr name="clipOuter" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ApngImageView">
        <attr name="apng_autoPlay" format="boolean" />
        <attr name="apng_loop" format="boolean" />
        <attr name="apng_rawRes" format="reference" />
    </declare-styleable>

    <declare-styleable name="CollapsingConstraintLayout">
        <attr name="appBarLayoutId" format="reference" />
    </declare-styleable>

    <declare-styleable name="Alpha">
        <attr name="interpolator" />
        <attr name="reverse" format="boolean" />
    </declare-styleable>

</resources>
