<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CheckedImageView">
        <!-- Indicates the initial checked state of this image. -->
        <attr name="checked" format="boolean" />
    </declare-styleable>

    <declare-styleable name="InterpolatorView">
        <attr name="interpolator" format="reference" />
    </declare-styleable>

    <declare-styleable name="AnimatedBorderView">
        <attr name="borderWidth" format="dimension" />
    </declare-styleable>

</resources>
